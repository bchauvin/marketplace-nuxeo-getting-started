<!-- See documentation at http://doc.nuxeo.com/x/HgIz -->
<package type="addon" name="nuxeo-platform-getting-started" version="@VERSION@">
  <title>Getting Started With Nuxeo Platform Package</title>
  <description>
    <p>This addon adds content and configuration to follow the "Getting Started With Nuxeo Platform" tutorial.</p>
  </description>
  <home-page>http://doc.nuxeo.com/</home-page>
  <vendor>Nuxeo</vendor>
  <installer restart="true" />
  <uninstaller restart="true" />
  <nuxeo-validation>nuxeo_certified</nuxeo-validation>
  <production-state>production_ready</production-state>
  <supported>false</supported>
  <platforms>
    <platform>cap-@DISTRIB_VERSION@</platform>
    <platform>cap-@DISTRIB_VERSION@-HF*</platform>
  </platforms>
  <dependencies>
    <package>nuxeo-dam</package>
  </dependencies>
  <license>Apache License v2.0</license>
  <license-url>http://www.apache.org/licenses/LICENSE-2.0.html</license-url>
  <visibility>PUBLIC</visibility>
</package>
